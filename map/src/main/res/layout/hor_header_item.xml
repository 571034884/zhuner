<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardPreventCornerOverlap="false"
    android:id="@+id/cardView"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/y48"
        android:paddingLeft="@dimen/x24"
        android:paddingRight="@dimen/x24"
        android:paddingBottom="@dimen/y10"
        android:orientation="vertical">
        <com.aibabel.map.views.AutoNextLineLinearlayout
            android:id="@+id/al_name"
            android:layout_width="match_parent"
            android:layout_marginBottom="@dimen/y24"
            android:layout_height="@dimen/y40">
        </com.aibabel.map.views.AutoNextLineLinearlayout>
        <TextView
            android:id="@+id/tv_details"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="40分钟 · 7站 · 学知园上车"
            android:textColor="@color/c_66"
            android:textSize="@dimen/y22"
            android:layout_marginBottom="@dimen/y32"
            />
        <LinearLayout
            android:id="@+id/ll_point"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal" >
        </LinearLayout>


    </LinearLayout>
</android.support.v7.widget.CardView>