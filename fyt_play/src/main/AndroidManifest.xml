        <meta-data
            android:name="design_height_in_dp"
            android:value="640" />
        <meta-data android:name="android.webkit.WebView.EnableSafeBrowsing"
            android:value="true" />
        <!-- 阿里热修复 -->
        <meta-data
            android:name="com.taobao.android.hotfix.IDSECRET"
            android:value="25544433-1" />
        <meta-data
            android:name="com.taobao.android.hotfix.APPSECRET"
            android:value="5aee3057e5093a27c604c1df74ead2c9" />
        <meta-data
            android:name="com.taobao.android.hotfix.RSASECRET"
            android:value="" />

        <activity
            android:name=".activity.ChoiceCityActivity"
            android:label="@string/title_activity_choice_city"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.WebActivity" />
        <activity android:name=".activity.MyWebActivity"
            android:exported="true"></activity>
