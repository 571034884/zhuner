<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MyWebActivity">
    <com.aibabel.fyt_play.js.AdvancedWebView
        android:id="@+id/wb"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.aibabel.fyt_play.js.AdvancedWebView>
    <com.aibabel.fyt_play.utils.EmptyLayout
        android:id="@+id/el_error"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.aibabel.fyt_play.utils.EmptyLayout>
    <ImageView
        android:id="@+id/iv_close"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/tq_guanbi"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="@dimen/x20"
        android:layout_marginTop="@dimen/y20"/>
</android.support.constraint.ConstraintLayout>